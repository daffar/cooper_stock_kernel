-			     WM8994_AIF1DRC1_SIG_DET_MODE |
-			     WM8994_AIF1DRC1_QR |
-			     WM8994_AIF1DRC1_ANTICLIP |
-			     WM8994_AIF1ADC1L_DRC_ENA);
-		wm8994_write(codec, WM8994_AIF1_DRC1_2, 0x0845);
-		wm8994_write(codec, WM8994_AIF1_DRC1_3, 0x0019);
