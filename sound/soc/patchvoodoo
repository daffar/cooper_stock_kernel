-   msleep( 10 );
-   
-   wm8994_write(codec, 0x60, 0x0022 );
-   wm8994_write(codec, 0x54, 0x0033 );
-   msleep( 200 );
-   wm8994_write(codec, 0x60, 0x00EE );
-
