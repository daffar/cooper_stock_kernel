-#define TUNE_CALL_RCV_MAIN_MIC_VOL		0x0B	// 0x18
-#define TUNE_CALL_RCV_RCV_VOL				0x39	// 0x20, 0x21
-
-#define TUNE_CALL_SPK_SUB_MIC_VOL			0x0E	// 0x18
-#define TUNE_CALL_SPK_SPK_VOL				0x3C	// 0x26, 0x27
-#define TUNE_CALL_SPK_SPK_CLASSD			0x2		// 0x25
