-      val |= (WM8994_AIF1ADC1_VU | 0xE5); // 0dB
-      wm8994_write(codec,WM8994_AIF1_ADC1_LEFT_VOLUME, val);
-   }
-   else
-*/   
-   {
-      //-------------------------------------- Analog Input
-      // Unmute IN1L PGA, update volume
