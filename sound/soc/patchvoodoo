-int wm8994_disable_path(struct snd_soc_codec *codec, int mode)
-{
-   int SndMethod = mode & 0xF0;
-   int SndDevice = mode & 0x0F;
-   
-   printk("%s() method = 0x%02x, device = 0x%02x\n",__func__,SndMethod, SndDevice);
