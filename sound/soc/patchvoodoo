-   {
-      wm8994_write(codec, 0x410, 0x1800); // Filter
-   
-      //-------------------------------------- Analog Input
-      // Unmute IN1L PGA, update volume
-      val = wm8994_read(codec,WM8994_LEFT_LINE_INPUT_1_2_VOLUME );   
