-   val |= (WM8994_SPKOUT_VU | 0x3F);
-   wm8994_write(codec, WM8994_SPEAKER_VOLUME_LEFT, val);
-   val = wm8994_read(codec, WM8994_SPEAKER_VOLUME_RIGHT);
-   val &= ~(WM8994_SPKOUT_VU_MASK | WM8994_SPKOUTR_VOL_MASK);
-   val |= (WM8994_SPKOUT_VU | 0x3F);
