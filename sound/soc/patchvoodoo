-   val = wm8994_read(codec, WM8994_AIF1_DAC1_RIGHT_VOLUME ); 
-   val &= ~(WM8994_AIF1DAC1_VU_MASK | WM8994_AIF1DAC1R_VOL_MASK);
-   val |= (WM8994_AIF1DAC1_VU | 0xC0); //0 dB volume  
-   wm8994_write(codec,WM8994_AIF1_DAC1_RIGHT_VOLUME, val);
-   // DAC1_VOL 2nd step
