-{
-	// global kill switch
-	if (!enable)
-		return;
-
-	if (recording_preset == 0)
-		return;
-
-	origin_recgain = wm8994_read(codec, WM8994_LEFT_LINE_INPUT_1_2_VOLUME);
-	origin_recgain_mixer = wm8994_read(codec, WM8994_INPUT_MIXER_3);
-	update_recording_preset(false);
-}
-#endif
