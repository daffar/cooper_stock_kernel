-	switch (clk_id) {
-	case WM8994_SYSCLK_MCLK:
-		wm8994->mclk_rate = freq;
-		wm8994->sysclk_source = clk_id;
-		break;
-	case WM8994_SYSCLK_FLL:
-		wm8994->sysclk_rate = freq;
-		wm8994->sysclk_source = clk_id;
-		break;
-
-	default:
-		return -EINVAL;
-	}
-
-	return 0;
