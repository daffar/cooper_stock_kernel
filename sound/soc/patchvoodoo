-   val &= ~(WM8994_AIF1ADCL_SRC_MASK|WM8994_AIF1ADCR_SRC_MASK);
-   wm8994_write(codec,WM8994_AIF1_CONTROL_1,val);
-
-/* ==================== Set default Gain ==================== */
-/*
