-   wm8994_write(codec,WM8994_AIF1_DAC1_EQ_GAINS_1, val);
-
-   val = wm8994_read(codec, WM8994_AIF1_DAC2_EQ_GAINS_1 );
-   val &= ~(WM8994_AIF1DAC2_EQ_ENA_MASK);
-   wm8994_write(codec,WM8994_AIF1_DAC2_EQ_GAINS_1, val);
