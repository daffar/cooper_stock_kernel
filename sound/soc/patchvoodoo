-   // Unmute the AF1DAC1
-   val = wm8994_read(codec, WM8994_AIF1_DAC1_FILTERS_1 );   
-   val &= ~(WM8994_AIF1DAC1_MUTE_MASK);
-   val |= WM8994_AIF1DAC1_UNMUTE;
